#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yuyang/blog/node_modules/.pnpm/vitepress@1.3.1_@algolia+client-search@4.24.0_postcss@8.4.39_sass@1.77.8_search-insights@2.15.0/node_modules/vitepress/bin/node_modules:/Users/yuyang/blog/node_modules/.pnpm/vitepress@1.3.1_@algolia+client-search@4.24.0_postcss@8.4.39_sass@1.77.8_search-insights@2.15.0/node_modules/vitepress/node_modules:/Users/yuyang/blog/node_modules/.pnpm/vitepress@1.3.1_@algolia+client-search@4.24.0_postcss@8.4.39_sass@1.77.8_search-insights@2.15.0/node_modules:/Users/yuyang/blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yuyang/blog/node_modules/.pnpm/vitepress@1.3.1_@algolia+client-search@4.24.0_postcss@8.4.39_sass@1.77.8_search-insights@2.15.0/node_modules/vitepress/bin/node_modules:/Users/yuyang/blog/node_modules/.pnpm/vitepress@1.3.1_@algolia+client-search@4.24.0_postcss@8.4.39_sass@1.77.8_search-insights@2.15.0/node_modules/vitepress/node_modules:/Users/yuyang/blog/node_modules/.pnpm/vitepress@1.3.1_@algolia+client-search@4.24.0_postcss@8.4.39_sass@1.77.8_search-insights@2.15.0/node_modules:/Users/yuyang/blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress.js" "$@"
fi
